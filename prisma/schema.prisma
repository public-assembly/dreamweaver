generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id        String @id
  address   String
  eventType String
  tags      Json
}

model CreatePress721 {
  id              String  @id
  newPress        String
  initialOwner    String
  initialLogic    String
  creator         String
  initialRenderer String
  soulbound       Boolean
}

model RendererUpdated {
  id          Int      @id @default(autoincrement())
  targetPress String
  renderer    String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model PressInitialized {
  id          Int      @id @default(autoincrement())
  targetPress String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  sender      String
}

model LogicUpdated {
  id          Int      @id @default(autoincrement())
  targetPress String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  logic       String
}

model DataStored {
  id          Int      @id @default(autoincrement())
  targetPress String
  storeCaller String
  pointer     String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  tokenId     BigInt
}
