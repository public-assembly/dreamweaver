generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// a transaction model that maps to a database table. This will be a blueprint for 'Transaction' entries in your PostgreSQL database.

model Transaction {
  id        String @id
  address   String
  eventType String
  tags      Json
}

model CreatePress721 {
  id            String @id
  newPress      String
  initialOwner  String
  initialLogic  String
  creator       String
  initialRenderer String
  soulbound     Boolean
}
