generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id        String @id
  address   String
  eventType String
  tags      Json
}

model RendererUpdated {
  targetPress String
  renderer    String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  id          Int      @id @default(autoincrement())
}

model PressInitialized {
  targetPress String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  sender      String
  Id          Int      @id @default(autoincrement())
}

model LogicUpdated {
  targetPress String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  logic       String
  id          Int      @id @default(autoincrement())
}

model DataStored {
  targetPress String
  storeCaller String
  pointer     String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  tokenId     BigInt
  id          Int      @id @default(autoincrement())
}

model Create721Press {
  newPress        String
  initialOwner    String
  initialLogic    String
  creator         String
  initialRenderer String
  soulbound       Boolean
  id              Int     @id @default(autoincrement())
}
